    "styled-components": "4.4.0",
    "validator": "10.9.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "chai-nightwatch": "0.4.0",
    "conventional-changelog-cli": "^2.0.11",
    "shelljs": "^0.7.4"
  },
