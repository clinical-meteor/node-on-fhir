    "@material-ui/pickers": "3.2.8",
    "@material-ui/styles": "4.9.0",
    "chart.js": "2.8.0",
    "classnames": "2.2.6",
    "clsx": "1.0.4",
    "date-fns": "2.8.1",
    "fhirclient": "2.1.0",
    "fhir-kit-client": "1.4.0",
