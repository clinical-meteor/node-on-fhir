    "meteor-node-stubs": "0.4.1",
    "moment": "2.22.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-icons-kit": "1.3.1",
    "react-mixin": "4.0.0",
    "react-multi-carousel": "^2.5.5",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "register-service-worker": "1.6.2",
    "setimmediate": "1.0.5",
    "simpl-schema": "1.5.3",
