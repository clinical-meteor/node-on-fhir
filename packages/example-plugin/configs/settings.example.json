        "ImagingStudies": true,
        "Immunizations": true,
        "Locations": true,
        "Lists": true,
        "Medications": true,
        "MedicationOrders": true,
        "MedicationStatements": true,
        "Organizations": true,
        "Observations": true,
        "Patients": true,
